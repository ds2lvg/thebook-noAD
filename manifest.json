{
  "name": "thebook-noAD",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "thebook에서 보기 짜증나는 광고 다 가리기",
  "background": {
      "scripts": [
          "init.js"
      ],
      "persistent": true
  },
  "browser_action": {
      "default_title": "No google AD!",
      "default_icon": {
        "16": "images/ad.png",
        "32": "images/ad.png",
        "48": "images/ad.png",
        "128": "images/ad.png"
      }
  },
  "permissions": [
      "https://*/*",
      "http://*/*",
      "tabs"
  ]
}